{
  "name": "syncable-packages",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "biu": "biu",
    "build:client": "rimraf syncable-client/bld && tsc -p syncable-client",
    "build:server": "rimraf syncable-server/bld && tsc -p syncable-server",
    "build": "yarn build:client && yarn build:server",
    "patch:syncable": "cd syncable && npm --no-git-tag-version version patch",
    "patch:syncable-client": "cd syncable-client && npm --no-git-tag-version version patch",
    "patch:syncable-server": "cd syncable-server && npm --no-git-tag-version version patch",
    "patch": "yarn patch:syncable && yarn patch:syncable-client && yarn patch:syncable-server && git add */package.json && npm --force version patch",
    "publish:syncable": "cd syncable && npm publish",
    "publish:syncable-client": "cd syncable-client && npm publish",
    "publish:syncable-server": "cd syncable-server && npm publish",
    "publish-all": "npm run publish:syncable && npm run publish:syncable-client && npm run publish:syncable-server"
  },
  "devDependencies": {
    "biu": "^0.2.11",
    "lerna": "^2.9.0",
    "mobx": "^3.6.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.1",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.3.4",
    "vts": "^5.8.3"
  }
}
